{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "grpcRoute": {
        "ClusterId": "backboneCluster",
        "Match": {
          "Path": "/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "backboneCluster": {
        "HttpRequest": {
          "Version": "2",
          "VersionPolicy": "RequestVersionExact"
        },
        "Destinations": {
          "BackboneNode1": {
            "Address": "https://localhost:5001"
          },
          "BackboneNode2": {
            "Address": "https://localhost:5002"
          }
        },
        "LoadBalancingPolicy": "RoundRobin"
      }
    }
  }
}